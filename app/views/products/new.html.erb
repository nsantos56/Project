<h1>New Product</h1>
<%= form_for @product do |f|%>
  <div>
    <%= f.label :title%>
    <br>
    <%= f.text_field :title%>
  <div>
   <div>
    <%= f.label :description%>
    <br>
    <%= f.text_area :description%>
  <div>
   <div>
    <%= f.label :price%>
    <br>
    <%= f.text_field :price%>
  <div>
   <div>
    <%= f.label :stock_quantity%>
    <br>
    <%= f.text_field :stock_quantity%>
  <div>
  <div>
    <%= f.submit "Create Product" %>
  <div>
<% end%>